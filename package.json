  "type": "module",
  "bugs": {
    "url": "https://github.com/leafcode-se/alvestass.se/issues"
  },
  "homepage": "https://github.com/leafcode-se/alvestass.se#readme",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "bulma": "^1.0.4",
    "sass": "^1.93.2"
  }
}
