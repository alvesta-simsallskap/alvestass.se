{
  "name": "alvestass.se",
  "version": "1.0.0",
  "description": "Alvesta Sims√§llskap Website",
  "main": "eleventy.config.js",
  "scripts": {
    "eleventy:serve": "eleventy --serve",
    "eleventy:build": "eleventy",
    "sass:watch": "sass src/_scss/site.scss:_site/css/site.css --load-path=node_modules --watch --update --color",
    "sass:build": "sass src/_scss/site.scss:_site/css/site.css --load-path=node_modules --no-source-map --style=compressed",
    "start": "run-p -l sass:build sass:watch eleventy:serve",
    "build": "run-s -l eleventy:build sass:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leafcode-se/alvestass.se.git"
  },
