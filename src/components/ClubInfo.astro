---
import { getEntry, render } from 'astro:content';
import type { CollectionEntry } from 'astro:content';
import CardPicture from './CardPicture.astro';
const { id } = Astro.props;
const entry = await getEntry('clubInfo', id) as CollectionEntry<'clubInfo'>;
const { Content } = await render(entry);
---

<CardPicture id={id} title={entry?.data.title}>
  <Content />
</Card>